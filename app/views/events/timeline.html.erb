<div id="timeline"></div>

<%= content_for :head do %>
  <%= javascript_include_tag "timeline" %>
<% end %>

<p style="color: green"><%= notice %></p>

<h1>Events</h1>

<div id="events">
  <% @events.each do |event| %>
    <%= render event %>
    <p>
      <%= link_to "Show this event", event %>
    </p>
  <% end %>
</div>

<%= link_to "New event", new_event_path %>
