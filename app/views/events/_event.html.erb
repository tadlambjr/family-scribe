<div id="<%= dom_id event %>">
  <p>
    <strong>Name:</strong>
    <%= event.name %>
  </p>

  <p>
    <strong>Start:</strong>
    <%= event.start %>
  </p>

  <p>
    <strong>End:</strong>
    <%= event.end %>
  </p>

  <p>
    <strong>Is annual:</strong>
    <%= event.is_annual %>
  </p>

  <p>
    <strong>Color:</strong>
    <%= event.color %>
  </p>

  <p>
    <strong>Notes:</strong>
    <%= event.notes %>
  </p>

</div>
