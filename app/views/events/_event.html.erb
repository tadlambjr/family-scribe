<div id="<%= dom_id event %>">
  <p>
    <strong>Name:</strong>
    <%= event.name %>
  </p>

  <p>
    <strong>Start:</strong> <%= event.start %>
    <strong>End:</strong> <%= event.end %>
  </p>

  <% if event.notes %>
  <p>
    <strong>Notes:</strong>
    <%= event.notes %>
  </p>
  <% end %>

</div>
